{
  "name": "random-cjs",
  "version": "4.1.2",
  "description": "Seedable random number generator supporting many common distributions.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falkorclark/random-cjs.git"
  },
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "contributors": [
    "Falkor Clark <falkorclark@gmail.com>"
  ],
  "type": "commonjs",
  "module": "./dist/random.js",
  "main": "./dist/random.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/random.js",
    "./dist/*.js": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "npm run build",
    "build": "tsc -p tsconfig.json",
    "prebuild": "npm run clean",
    "clean": "del /f /q dist",
    "prepublishOnly": "npm run build",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "rng",
    "prng",
    "stats",
    "d3-random",
    "seedrandom",
    "distribution",
    "pseudorandom",
    "uniform",
    "normal",
    "gaussian",
    "lognormal",
    "poisson",
    "exponential",
    "irwinhall",
    "bates"
  ],
  "dependencies": {
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/seedrandom": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
